<h1>Add New Blog</h1>

<%= form_with scope: :blog, url: user_blogs_path(current_user), method: :post  do |form| %>
<div>
	<%= form.label :title %>
	<%= form.text_field :title %>
</div>
<div>
	<%= form.label :description %>
	<%= form.text_field :description %>
</div>
<div>
	<%= form.submit %>
</div>
<% end %>





